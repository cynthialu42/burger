<h1>Index page!</h1>
<ul>
    {{#each burger}}
    <li>
        {{burger_name}}
    </li>
    {{/each}}
</ul>


<h2>Add a Burger</h2>
<form class="create-form">
    <div class="form-group">
        <label for = "name">Burger Name: </label>
        <input type = "text" id = "name" name = "burger-name"> 
    </div>
    <button type = "submit">Add Burger</button>
</form>
